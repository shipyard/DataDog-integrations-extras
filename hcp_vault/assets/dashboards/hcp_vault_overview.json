                "layout_type": "ordered",
                "widgets": [
                    {
                        "id": 4705539663238666,
                        "definition": {
                            "title": "Sealed Status",
                            "title_size": "16",
                            "title_align": "left",
                            "type": "query_value",
                            "requests": [
                                {
                                    "formulas": [{"formula": "query1"}],
                                    "conditional_formats": [
                                        {"comparator": "<=", "palette": "white_on_red", "value": 0},
                                        {"comparator": ">", "palette": "white_on_green", "value": 0}
                                    ],
                                    "response_format": "scalar",
                                    "queries": [
                                        {
                                            "query": "sum:hcp.vault_core_unsealed{$cluster,$project_id}",
                                            "data_source": "metrics",
                                            "name": "query1",
                                            "aggregator": "last"
                                        }
                                    ]
                                }
                            ],
                            "autoscale": true,
                            "precision": 2
                        },
                        "layout": {"x": 0, "y": 0, "width": 4, "height": 2}
                    },
